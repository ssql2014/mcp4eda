# Authentication credentials for semiconductor platforms
# Copy this file to .env and fill in your credentials

# DesignReuse credentials
DESIGNREUSE_USERNAME=your_username
DESIGNREUSE_PASSWORD=your_password

# AnySilicon API (if available)
ANYSILICON_API_KEY=your_api_key

# Scraper options
SCRAPER_HEADLESS=true
SCRAPER_CACHE_TTL=900

# Rate limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=10